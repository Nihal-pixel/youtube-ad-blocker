{
    "manifest_version": 2,

    "name": "Youtube Ad blocker",
    "version": "1.0",

    "content_scripts": [{
        "js": ["script.js"],
        "run_at": "document_idle",
        "matches": ["<all_urls>"]
    }],

    "permissions": [
        "tabs",
        "activeTab"
    ]
}